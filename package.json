{
  "name": "express-mvc",
  "version": "1.0.0",
  "description": "example oop and mvc arcitecture express",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"},
  "repository": {
    "type": "git",
    "url": "restuwahyu13"
  },
  "keywords": [
    "express",
    "express-mvc",
    "mvc-pattern"
  ],
  "author": "resuwahyu13",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "heroku": "^7.60.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.9.1"
  },
  "devDependencies": {
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2"
  },
  "_moduleAliases": {
    "cores": "core/",
    "configs": "configs/",
    "controllers": "app_main/controllers/",
    "helpers": "app_main/helpers/",
    "middlewares": "app_main/middlewares/",
    "models": "app_main/models/",
    "routes": "app_main/routes",
    "views": "app_main/views/",
    "libs": "app_main/libs/"
  }
}
